{
  "name": "@ldesign/breadcrumb",
  "type": "module",
  "version": "0.1.0",
  "description": "LDesign 面包屑组件 - 现代化面包屑导航，支持路由自动生成、下拉菜单、折叠显示",
  "author": "LDesign Team",
  "license": "MIT",
  "keywords": [
    "ldesign",
    "breadcrumb",
    "navigation",
    "route",
    "vue",
    "typescript"
  ],
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./core": {
      "types": "./es/core/index.d.ts",
      "import": "./es/core/index.js",
      "require": "./lib/core/index.cjs"
    },
    "./vue": {
      "types": "./es/vue/index.d.ts",
      "import": "./es/vue/index.js",
      "require": "./lib/vue/index.cjs"
    },
    "./vue/styles": "./packages/vue/src/styles/index.css",
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "types": "./es/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "es",
    "lib",
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "dev": "ldesign-builder build --mode development --watch",
    "clean": "rimraf es lib dist coverage",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    }
  },
  "dependencies": {
    "@ldesign/shared": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@ldesign/builder": "workspace:*",
    "eslint": "^9.18.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.7.3"
  }
}

